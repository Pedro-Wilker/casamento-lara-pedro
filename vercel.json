{
  "builds": [
    {
      "src": "src/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/assets/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "src/css/**/*",
      "use": "@vercel/static"
    }
  ],
  "outputDirectory": "src",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}